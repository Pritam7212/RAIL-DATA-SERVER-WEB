// Function to generate a random date within the past year
function getRandomDate() {
    const today = new Date();
    const pastYear = new Date(today.getFullYear() - 1, today.getMonth(), today.getDate());
    const randomDate = new Date(pastYear.getTime() + Math.random() * (today.getTime() - pastYear.getTime()));
    return randomDate.toISOString().split('T')[0];
}

// Function to generate a random hex error code between 00 and FF
function getRandomHexErrorCode() {
    const hexCode = Math.floor(Math.random() * 256).toString(16).toUpperCase();
    return hexCode.padStart(2, '0'); // Ensure it is always two digits
}

// Function to get an error description based on the error code
function getErrorDescription(errorCode) {
    const descriptions = {
        "00": "No Error",
        "01": "Minor Glitch",
        "FF": "Critical Failure",
        "AB": "Checksum Mismatch",
        "CD": "Configuration Error",
        "EE": "Memory Overflow",
        "7F": "Device Not Responding",
    };
    // Return a predefined description if exists, otherwise a generic message
    return descriptions[errorCode] || "Unknown Error";
}

